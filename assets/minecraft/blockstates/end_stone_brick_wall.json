{
    "multipart": [
        {   "when": { "up": "true" },
            "apply": { "model": "block/custom/crash_barrier/center_pole" }
        },
        {   "when": { "north": "low" },
            "apply": { "model": "block/custom/crash_barrier/crash_barrier_r", "y": 90, "uvlock": true }
        },
        {   "when": { "east": "low" },
            "apply": { "model": "block/custom/crash_barrier/crash_barrier_l", "uvlock": true }
        },
        {   "when": { "south": "low" },
            "apply": { "model": "block/custom/crash_barrier/crash_barrier_l", "y": 90, "uvlock": true }
        },
        {   "when": { "west": "low" },
            "apply": { "model": "block/custom/crash_barrier/crash_barrier_r", "uvlock": true }
        },
        {   "when": { "north": "tall" },
            "apply": { "model": "block/custom/crash_barrier/crash_barrier_r", "y": 90, "uvlock": true }
        },
        {   "when": { "east": "tall" },
            "apply": { "model": "block/custom/crash_barrier/crash_barrier_l", "uvlock": true }
        },
        {   "when": { "south": "tall" },
            "apply": { "model": "block/custom/crash_barrier/crash_barrier_l", "y": 90, "uvlock": true }
        },
        {   "when": { "west": "tall" },
            "apply": { "model": "block/custom/crash_barrier/crash_barrier_r", "uvlock": true }
        }
    ]
}
