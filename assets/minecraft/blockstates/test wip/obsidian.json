            "apply": { "model": "block/custom/bookshelf/bookshelf2", "y": 90}
        },
		{   "when": { "east": "true" },
            "apply": { "model": "block/oak_fence_side", "y": 90, "uvlock": true }
        },
		{   "when": { "south": "true" },
            "apply": { "model": "block/custom/bookshelf/bookshelf2", "y": 270}
        },
		{   "when": { "west": "true" },
            "apply": { "model": "block/oak_fence_side", "y": 270, "uvlock": true }
        }
	]
}
